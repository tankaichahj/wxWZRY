/**app.wxss**/
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  z-index: 2;
  background-color: #ffffff00;
  background:#ffffff00;
  
} 
.title {
  font-family: PingFang SC;
  font-weight: 500;
  color: rgb(255, 255, 255);
  font-size: 44rpx;
  margin-bottom: 40rpx;
}
/* 分割线 */
.separator {
  width: 95%;
  height: 2px;
  background-color: rgb(255, 255, 255); /* 设置分割线颜色 */
  margin: 5px 2px; /* 设置上下边距 */
}
button {
  background: initial;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}


page {
  padding-top: 54rpx;
  background-color: #000000a8;
  padding-bottom: 60rpx;
  background: radial-gradient(ellipse at bottom, #1b2735 0%, #090a0f 100%)
}
@keyframes moveUp1 {
  100% {
    transform: translateY(-100vh);
  }
}



